@using InplayBet.Web.Models
@using InplayBet.Web.Utilities
@model List<UserRankViewModel>
@{
    ViewBag.Title = "Ranking";
}
@section PageScript{
    @Html.RenderScripts("~/Scripts/Ranking")
}
@section PageStyle{
    @Html.RenderStyles("~/Styles/Ranking")
}
@{Html.RenderPartial("_PageTitle", new ViewDataDictionary { { "Caption", "Ranking" } });}
<div class="main-container">
    <div class="container">
        <div class="rank-pagger"></div>
        @{Html.RenderPartial("_UserRank", Model, new ViewDataDictionary { { "LastElement", 3 } });}
        <div class="rank-pagger"></div>
        @{Html.RenderPartial("_PromoBanner");}
    </div>
</div>
